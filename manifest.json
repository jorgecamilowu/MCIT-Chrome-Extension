{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MCIT Portal",
  "version": "1.0",
  "description": "Initial testings for MCIT Portal Chrome extension",
  "manifest_version": 2,
  "background": {
    "scripts": ["JavaScript/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logos/penn_favicon.png",
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html",
    "open_in_tab": false
  }
}
